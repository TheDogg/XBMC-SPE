<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Test Smart Playlist</name>
    <match>one</match>
    <rule field="playcount" operator="greaterthan">10</rule>
    
    <limit>50</limit>
    <order direction="ascending">random</order>
</smartplaylist>
